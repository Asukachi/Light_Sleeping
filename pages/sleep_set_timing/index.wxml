<import src="../../wxParse/wxParse.wxml" /> 
<view class="weui-cell" style="color: #00CCCC">
<view class="weui-cell__bd fontstyle">定时开关</view> 
<switch checked type="swicth" color="#00CCCC" style="zoom:1" class="wx-switch-input wx-switch-input::before wx-switch-input::after"></switch>
</view>
<view class="weui-cell" style="color: #00CCCC" type="button" bindtap="navigateTo" data-url="">
<view class="weui-cell__bd fontstyle">定时设置</view>
</view>

<view class="section" style="color: #00CCCC">
  <picker mode="time" value="{{time}}" start="00:01" end="24:00" bindchange="bindTimeChange">
    <view class="picker fontstyle2" style="display: flex; align-items: center; justify-content: center">
      {{time}}
    </view>
  </picker>
</view>

<image style="position: fixed; bottom: 50rpx; width: 300rpx; height: 300rpx; justify-content: flex-end; align-items: flex-end; right: 50rpx" src="/image/music.png" />