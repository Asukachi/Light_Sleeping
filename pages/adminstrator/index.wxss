page{background-color: #000033;}
.arrow{
  width: 10px;
  height: 10px;
  border-top: solid #00CCCC;
  border-right: solid #00CCCC;
  position: absolute;
  right: 20rpx;
  transform: rotate(45deg);
  margin-top:10rpx; 
}
.fontstyle{
  font-size: 20px;
}
.updown-image {
  width:100%;
  height: 200rpx;
  display: flex;
  justify-content: center;
}
 
.personal-img {
  width: 50%;
  height: 100%;
  display: flex;
  justify-content: center;
  margin-top: 10rpx;
}
 
.personal-img image {
  display: flex;
  justify-content: center;
  width: 270rpx;
  height:270rpx;
  background: #FFFFFF;
  border: 2rpx solid #E5E5E5;
  border-radius: 50%;
}

.personal-img image image{
  width: 100%;
  height: 100%;
  border-radius: 50%;
}